{
  "name": "w-lopes/poc-php-strict-types",
  "type": "poc",
  "description": "PHPCS sniff to enforce types",
  "require": {
    "squizlabs/php_codesniffer": "^3.3",
    "php": ">=7.1"
  },
  "require-dev": {
    "gskema/phpcs-type-sniff": "^0.15.0"
  },
  "autoload": {
    "psr-4": {
      "Gskema\\TypeSniff\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gskema\\TypeSniff\\": "tests/"
    }
  }
}
